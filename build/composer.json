{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for narrativestudies.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/bibcite": {
                "Fix BibTex carriage returns": "https://www.drupal.org/files/issues/fix-bibtext-cr-2875764-9.patch"
            },
            "drupal/node_edit_protection": {
                "Allow button tag": "https://www.drupal.org/files/issues/handle_button_tags.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unblibraries/narrativestudies.lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-dev-libs.git"
        }
    ],
    "require": {
        "academicpuma/citeproc-php": "~1.0",
        "audiolabs/bibtexparser": "dev-master",
        "ckeditor/ckeditor": "^4.5.11",
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "davidgorges/human-name-parser": "~0.2",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bibcite": "1.x-dev",
        "drupal/bootstrap": "~3.4",
        "drupal/ckeditor_markdown": "1.0",
        "drupal/ckeditor_media_embed": "1.0",
        "drupal/config_update": "1.3",
        "drupal/console": "~1.0",
        "drupal/core": "8.3.2",
        "drupal/ctools": "3.0-beta2",
        "drupal/editor_file": "1.2",
        "drupal/externalauth": "1.0",
        "drupal/features": "3.5",
        "drupal/field_collection": "3.x-dev",
        "drupal/ldap": "3.x-dev",
        "drupal/node_edit_protection": "1.x-dev",
        "drupal/override_node_options": "2.x-dev",
        "drupal/pathauto": "1.0",
        "drupal/sharethis": "2.0-beta3",
        "drupal/token": "1.0-rc1",
        "drush/drush": "~8.1.10",
        "technosophos/LibRIS": "~2.0",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unblibraries/drupal-dev-libs": "master"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
