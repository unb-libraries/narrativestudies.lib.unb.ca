{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of narrativestudies.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {}
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unb-libraries/narrativestudies.lib.unb.ca-drupal8",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        }
    ],
    "require": {
        "academicpuma/citeproc-php": "1.0",
        "audiolabs/bibtexparser": "dev-master",
        "ckeditor/ckeditor": "4.5.11",
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "1.6.3",
        "davidgorges/human-name-parser": "~0.2",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bibcite": "1.x-dev",
        "drupal/bootstrap": "3.14",
        "drupal/config_update": "1.5",
        "drupal/console": "~1.0",
        "drupal/core": "8.6.3",
        "drupal/ctools": "3.0-beta2",
        "drupal/externalauth": "1.1",
        "drupal/features": "3.7",
        "drupal/ldap": "3.0-beta3",
        "drupal/override_node_options": "2.x-dev",
        "drupal/pathauto": "1.2",
        "drush/drush": "^8.1.15",
        "technosophos/LibRIS": "~2.0",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
